<%- include("header"); -%>
    <div id="heading" class="box">
        <h1 ><%= listTitle %> - To Do List</h1>  
    </div>
    <div class="box">
        <% for(i=0; i<items.length; i++){%>
        <div class="item">
                <input type="checkbox">
                <p><%= items[i] %></p>
        </div>
        <%}%>
        <form class="item" action=<%=route%> method="POST">
            <input autocomplete="off" name="newItem" type="text" placeholder="New To Do">
            <button type="submit" name="list" value=<%=route%>>+</button>
        </form>
    </div>
<%- include("footer"); -%>